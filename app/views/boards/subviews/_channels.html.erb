<%board.channels.each_with_index do |channel, i|%>
    <div class="thumbnail span1">
      <a href=<%="#{edit_channel_path(channel)}"%> class="thumbnail" rel="tooltip" data-original-title="Edit channel <%=i+1%>, <%=channel.name%>" data-placement="bottom">
        <%= image_tag("#{channel.channel_image_url(:rec)}", :class => "img-responsive", :alt =>"Responsive image") %>
      </a>
    </div>
<%end%>