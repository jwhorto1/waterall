<div class="page-header">
  <h1>Recent Configuration <small> Activity listed in seconds</small></h1>
</div>


<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Channel 1</th>
      <th>Channel 2</th>
      <th>Channel 3</th>
      <th>Channel 4</th>
      <th>Channel 5</th>
      <th>Channel 6</th>
      <th>Channel 7</th>
      <th>Channel 8</th>
      <th>Date</th>
      <!-- <th></th>
      <th></th> -->
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @boardshortmessages.each do |boardshortmessage| %>
      <tr>
        <td><span class="label label-success"><%= boardshortmessage.channel1_on_in_seconds %></span></td>
        <td><span class="label label-success"><%= boardshortmessage.channel2_on_in_seconds %></span></td>
        <td><span class="label label-success"><%= boardshortmessage.channel3_on_in_seconds %></span></td>
        <td><span class="label label-success"><%= boardshortmessage.channel4_on_in_seconds %></span></td>
        <td><span class="label label-success"><%= boardshortmessage.channel5_on_in_seconds %></span></td>
        <td><span class="label label-success"><%= boardshortmessage.channel6_on_in_seconds %></span></td>
        <td><span class="label label-success"><%= boardshortmessage.channel7_on_in_seconds %></span></td>
        <td><span class="label label-success"><%= boardshortmessage.channel8_on_in_seconds %></span></td>
        <td><span class="label"><%= boardshortmessage.date.to_formatted_s(:long) %></span></td>
        <!-- <td><%= link_to 'Show', boardshortmessage %></td> -->
        <td><%= link_to 'Edit', edit_boardshortmessage_path(boardshortmessage) %></td>
        <!-- <td><%= link_to 'Delete', boardshortmessage, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>
<br>


