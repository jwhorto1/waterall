  </thead>
  <tbody>
      <tr>
        <td><input type="text"></input></td>
        <td><button class="btn">Left</button></td>
        <td><button class="btn">Left</button></td>
        <td><button class="btn">Left</button></td>
        <td><button class="btn">Left</button></td>
        <td><button class="btn">Left</button></td>
        <td><button class="btn">Left</button></td>
        <td><button class="btn">Left</button></td>
        <td><button class="btn">Left</button></td>
      </tr>
  </tbody>
</table>
<div class="btn-toolbar">
  <div class="btn-group">
